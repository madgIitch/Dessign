                $('.inner-content').show();
                var wrapper = $('#all-projects-wrapper').height();
                $('.column-2').css('min-height', wrapper-20);
            }
            else{
                $('#toggleAll').text('open all');
                $('.inner-content').hide();
                $('.column-2').css('min-height', '');
                var wrapper = $('#all-projects-wrapper').height();
                $('.column-2').css('min-height', wrapper-20);
            }
        }
        
        
        function moreImages(currButton){
            if(!$(currButton).hasClass('loaded')){
                loadImages(currButton);
                $(currButton).addClass('loaded');
            }

            if($(currButton).parent().parent().find('.image-plus').hasClass('hidden')){
                $(currButton).parent().parent().find('.image-plus').show();
                $(currButton).parent().parent().find('.caption').show();
                $(currButton).parent().parent().find('.image-plus').removeClass('hidden');
                $(currButton).text('Less images');
                setTimeout(function(){
                    heightToggle();
                },200)
                setTimeout(function(){
                    heightToggle();
                },500)
                setTimeout(function(){
                    heightToggle();
                },1000)
                
            }else{
                $(currButton).parent().parent().find('.image-plus').addClass('hidden');
                $(currButton).parent().parent().find('.image-plus').hide();
                $(currButton).parent().parent().find('.caption').hide();
                $(currButton).text('More images');
                
                setTimeout(function(){
                    heightToggle();
                },200)
                setTimeout(function(){
                    heightToggle();
                },500)
                setTimeout(function(){
                    heightToggle();
                },1000)
                
                
            }
            
            

        }
        
        
